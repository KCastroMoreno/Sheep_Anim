    R_Eyes = Ch_grp+'|Body|expression|Eye|R_eye|R_eye_Normal', Ch_grp+'|Body|expression|Eye|R_eye|R_eye_Sad', Ch_grp+'|Body|expression|Eye|R_eye|R_eye_Angry', Ch_grp+'|Body|expression|Eye|R_eye|R_eye_TopLid', Ch_grp+'|Body|expression|Eye|R_eye|R_eye_BotLid'
    for I in R_Eyes:
        cmds.setAttr(I+'.translateZ', -1)
        cmds.setAttr(I+'.translateY', 0)
        cmds.setAttr(I+'.translateX', 0)
    cmds.setAttr(R_Eyes[3]+'.translateZ', 0)
    cmds.setAttr(R_Eyes[3]+'.translateY', 0)
    cmds.setAttr(R_Eyes[3]+'.translateX', 0)

def L_eye_TopLid():
    Ch_grp = cmds.textField('b2', q = 1, tx=1)
    L_Eyes = Ch_grp+'|Body|expression|Eye|L_eye|L_eye_Normal', Ch_grp+'|Body|expression|Eye|L_eye|L_eye_Sad', Ch_grp+'|Body|expression|Eye|L_eye|L_eye_Angry', Ch_grp+'|Body|expression|Eye|L_eye|L_eye_TopLid', Ch_grp+'|Body|expression|Eye|L_eye|L__eye_BotLid'
    for I in L_Eyes:
        cmds.setAttr(I+'.translateZ', -1)
        cmds.setAttr(I+'.translateY', 0)
        cmds.setAttr(I+'.translateX', 0)
    cmds.setAttr(L_Eyes[3]+'.translateZ', 0)
    cmds.setAttr(L_Eyes[3]+'.translateY', 0)
    cmds.setAttr(L_Eyes[3]+'.translateX', 0)

def R_eye_BotLid():
    Ch_grp = cmds.textField('b2', q = 1, tx=1)
    R_Eyes = Ch_grp+'|Body|expression|Eye|R_eye|R_eye_Normal', Ch_grp+'|Body|expression|Eye|R_eye|R_eye_Sad', Ch_grp+'|Body|expression|Eye|R_eye|R_eye_Angry', Ch_grp+'|Body|expression|Eye|R_eye|R_eye_TopLid', Ch_grp+'|Body|expression|Eye|R_eye|R_eye_BotLid'
