def SelectMouth():
    
    Ch_grp = cmds.textField('b2', q = 1, tx=1)
    Last = cmds.textField('a10', q = 1, tx=1)
    Mid = '|Body|expression|Mouth|'
    
    cmds.select(Ch_grp +Mid + Last)



def MouthEX_OFull():
    Ch_grp = cmds.textField('b2', q = 1, tx=1)
    Mouth = Ch_grp+'|Body|expression|Mouth|Open|O_Full', Ch_grp+'|Body|expression|Mouth|Open|O_Half', Ch_grp+'|Body|expression|Mouth|Open|O_surprise', Ch_grp+'|Body|expression|Mouth|Open|Sad', Ch_grp+'|Body|expression|Mouth|Open|Happy',Ch_grp+'|Body|expression|Mouth|Neutral|Normal', Ch_grp+'|Body|expression|Mouth|Neutral|Happy', Ch_grp+'|Body|expression|Mouth|Neutral|Semi_Happy', Ch_grp+'|Body|expression|Mouth|Neutral|Sad',Ch_grp+'|Body|expression|Mouth|Neutral|Semi_Sad', Ch_grp+'|Body|expression|Mouth|Neutral|R_Uhm', Ch_grp+'|Body|expression|Mouth|Neutral|L_Uhm',Ch_grp+'|Body|expression|Mouth|Teeth|Normal', Ch_grp+'|Body|expression|Mouth|Teeth|Happy', Ch_grp+'|Body|expression|Mouth|Teeth|Sad'
    
    for I in Mouth:
        cmds.setAttr(I+'.translateZ', -1)
        cmds.setAttr(I+'.translateY', 0)
        cmds.setAttr(I+'.translateX', 0)
    
    cmds.setAttr(Mouth[0]+'.translateZ', 0)
    cmds.setAttr(Mouth[0]+'.translateY', 0)
    cmds.setAttr(Mouth[0]+'.translateX', 0)
    
    Split = Mouth[0].split('|')
    Name = str(Split[-2])+'|'+str(Split[-1])
    cmds.textField('a10', edit=True, tx=Name)
    

