import pymel.core as pm
import maya.cmds as cmds

def playF():
    pm.mel.playButtonStepForward()

def playB():
    pm.mel.playButtonStepBackward()

def SetKeys():
    cmds.setKeyframe()

def ClearKey():
    pm.mel.timeSliderClearKey()
  
#-------------------------------------------------

def GetCh():
    cmds.refresh()
    shapes = cmds.ls(sl=1)
    cmds.textField('b2', edit=True, tx=shapes[0])


def ParentALL():
    Ch_grp = cmds.textField('b2', q = 1, tx=1)


    COB = Ch_grp + '|Global_control|Torso_control',Ch_grp + '|Body|body',Ch_grp + '|Body|expression',Ch_grp + '|Body|ears',Ch_grp + '|Body|horns',Ch_grp + '|Global_control|L_leg_control',Ch_grp + '|Global_control|R_leg_control',Ch_grp + '|Body|legs|L_leg', Ch_grp + '|Body|legs|R_leg'
    
    
    cmds.select(COB[0],COB[1])
    cmds.ParentConstraint()
    
    cmds.select(COB[0],COB[2])
    cmds.ParentConstraint()
    
    cmds.select(COB[0],COB[3])
    cmds.ParentConstraint()
    
    cmds.select(COB[0],COB[4])
    cmds.ParentConstraint()
    
    #- 
    cmds.select(COB[5],COB[7])
    cmds.ParentConstraint()
    
    cmds.select(COB[6],COB[8])
    cmds.ParentConstraint()
    
    #- 
    
    
    cmds.setAttr(Ch_grp+'|Body|arms|R_arm.inheritsTransform', 0)
    cmds.setAttr(Ch_grp+'|Body|arms|L_arm.inheritsTransform', 0)
    
    cmds.select(Ch_grp+'|Global_control|Torso_control|R_armjoint|R_armjoint_01')
    cmds.select(Ch_grp + '|Body|arms|R_arm', add = True)
    cmds.bindSkin()

    cmds.select(Ch_grp+'|Global_control|Torso_control|L_armjoint|L_armjoint_01')
    cmds.select(Ch_grp + '|Body|arms|L_arm', add = True)
    cmds.bindSkin()

#----------------------------------
